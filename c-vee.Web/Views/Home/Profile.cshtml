            </div>

            <div class="col-lg-6 col-sm-12 info shadow-sm">
                <h2>
                    @Model.Data.FirstName @Model.Data.LastName
                </h2>
                <h4>
                    @Model.Data.jobTitle
                </h4>
                <p>
                    <span>@Model.Data.email</span><span class="px-3"> @Model.Data.Phone</span>
                </p>
            </div>
        </div>
    </div>

